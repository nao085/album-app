!!!
%html
  %head
    %meta{:content => "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}/
    %title AlbumApp
    = csrf_meta_tags
    = csp_meta_tag
    = stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload'
    %link{href: "https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.css", rel: "stylesheet", type: "text/css"}/
    = javascript_pack_tag 'application', 'data-turbolinks-track': 'reload'
    %script{src: "https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js", type: "text/javascript"}
    %script{:src => "https://ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js"}
    %script{:src => "https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.20.1/moment.min.js"}
    %script{:src => "https://cdnjs.cloudflare.com/ajax/libs/livestamp/1.1.0/livestamp.js"}
  %body
    = render partial: 'layouts/flash'
    = yield
